{
  "name": "campus-flow-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.gatech.edu/pages/cx4230-sp22-group1/campus-flow/",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-p5-wrapper": "^3.2.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "5.0.0",
    "tailwindcss": "^2.2.7",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "concurrently --restart-tries 5 --names \"  CSS,REACT,  MAP\" \"npm run watch:css\" \"react-scripts start\" \"npm run watch:maps\"",
    "build": "npm run build:css && npm run build:maps && react-scripts build",
    "postbuild": "ncp build\\index.html build\\404.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/styles/tailwind.css -o src/assets/styles/index.css",
    "watch:css": "nodemon --watch src/assets/styles --watch tailwind.config.js --ignore src/assets/styles/index.css --ext scss,css --exec \"npm run build:css\"",
    "build:maps": "python preprocessing/parse_map.py --maps src/assets/maps",
    "watch:maps": "nodemon --watch preprocessing --watch src/maps --ext py,png --exec \"npm run build:maps\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "gh-pages": "^3.2.3",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.6",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0"
  }
}
